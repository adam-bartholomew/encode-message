{
  "version": 2,
  "builds": [
      {
          "src": "app.py",
          "use": "@vercel/python"
      },
      {
        "src": "/static/(.*)",
        "dest": "@vercel/static"
      }
  ],
  "routes": [
      {
          "src": "(.*)",
          "dest": "app.py"
      }
  ],
  "trailingSlash": true
}
